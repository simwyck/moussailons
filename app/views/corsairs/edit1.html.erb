<h1>Edit Corsair</h1>

<%= form_for @corsair do |f| %>

<div class="form-group">

  <label>First Name</label>

  <%= f.text_field :first_name, class: 'form-control' %>

</div>

<div class="form-group">

  <label>Age</label>

  <%= f.text_field :age, class: 'form-control' %>

</div>

<div class="form-group">

  <button type="submit">Modify Corsair</button>

</div>

<% end %>
